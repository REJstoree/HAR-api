{
  "version": 2,
  "installCommand": "npm install --legacy-peer-deps",
  "buildCommand": "echo 'No build required'",
  "framework": null,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}